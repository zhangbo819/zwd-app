{
  "name": "zwd",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint .",
    "android": "react-native run-android",
    "ios": "react-native run-ios --configuration Debug",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/gxb/main.jsbundle --assets-dest ./ios",
    "release-android": "cd android && sh gradlew assembleRelease && sh ./app/afterBuild/afterBuild.sh && open ./app/build/outputs/apk/",
    "link": "react-native link",
    "unlink": "react-native unlink",
    "output-ios": "react-native bundle --entry-file index.js --bundle-output ./dist/index.ios.jsbundle --platform ios --assets-dest ./dist --dev false"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "^0.59.2",
    "react-native-device-info": "^1.4.1",
    "react-native-storage": "^1.0.1",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^2.9.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
